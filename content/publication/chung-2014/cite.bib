 doi = {10.1109/MWP.2014.6994559},
 file = {PDF:/Users/hanc/Zotero/storage/8BHJ34AQ/06994559.pdf:application/pdf},
 isbn = {978-4-88552-290-1},
 keywords = {IFoF, analog transmission, CNR, LTE/LTE-A, Radio over fiber (RoF)},
 pages = {303--306},
 publisher = {IEEE},
 title = {Design of RoF based Mobile Fronthaul Link with Multi-IF Carrier for LTE/LTE-A Signal Transmission},
 year = {2014}
}
